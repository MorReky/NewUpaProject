<Page x:Class="UpaProject.Views.Equipments.PageAsuEqEdit"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:UpaProject.Views.Equipments"
      xmlns:vm1="clr-namespace:UpaProject.ViewModels.Base"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Редактирование единиц оборудования">
    <Page.DataContext>
        <vm1:PageAsuEqEditViewModelcs/>
    </Page.DataContext>
    <DockPanel LastChildFill="False">
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal"
                    Margin="5">
            <TextBlock Style="{StaticResource Txt}"
                       MinWidth="150"
                       Text="Номер ЕО:"/>
            <TextBox Name="TxbEO" Width="200"
                     IsReadOnly="True"/>
        </StackPanel>
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal"
                    Margin="5">
            <TextBlock Style="{StaticResource Txt}"
                       MinWidth="150"
                       Text="Наименование ЕО:"/>
            <TextBox Name="TxbEOName" Width="200"
                     IsReadOnly="True"/>
        </StackPanel>
    </DockPanel>
</Page>
